{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "images/*":[
        "public/images/*"
      ],
      "@/*": [
        "src/*"
      ],
      "components/*":[
        "src/components/*"
      ],
      "views/*":[
        "src/views/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  }
}
